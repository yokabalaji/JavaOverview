package Lms;

public class HowManyEvenNumOdd {
	public static void main(String[] args) {
		String a=args[0];
		String b[]=a.split(",");
		int d=Integer.parseInt(b[0]);
		int e=Integer.parseInt(b[1]);
		int f=Integer.parseInt(b[2]);
		int g=Integer.parseInt(b[3]);
		int c[]= {d,e,f,g};
		int count=0;
		int count1=0;
		for(int i=0;i<c.length;i++) {
			if(i%2==0) {
				count++;
			}else {
				count1++;
			}
		}
		System.out.println("odd count="+count+"even count="+count1);
	}

}
